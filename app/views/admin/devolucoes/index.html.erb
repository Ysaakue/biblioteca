<p id="notice"><%= notice %></p>

<h1>Devolucoes</h1>

<table>
  <thead>
    <tr>
      <th>Aluno</th>
      <th>Bibliotecario</th>
      <th>Multa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devolucoes.each do |devolucao| %>
      <tr>
        <td><%= devolucao.aluno %></td>
        <td><%= devolucao.bibliotecario %></td>
        <td><%= devolucao.multa %></td>
        <td><%= link_to 'Show', devolucao %></td>
        <td><%= link_to 'Edit', edit_devolucao_path(devolucao) %></td>
        <td><%= link_to 'Destroy', devolucao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Devolucao', new_admin_devolucao_path %>
