<%= simple_form_for(@exemplar) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :livro %>
    <%= f.association :emprestimo %>
    <%= f.input :aquisicao %>
    <%= f.input :situacao %>
    <%= f.input :em_emprestimo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
